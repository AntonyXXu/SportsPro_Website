@model Technician
@{
    ViewData["Title"] = "Update Incidents";
}

<h1>Select Technician</h1>

<form asp-action="TechList" asp-route-id="" method="get">
    <div class="form-group">
        <label asp-for="TechnicianID" class="control-label"></label>
        <select asp-for="TechnicianID" class="form-control"
                asp-items="@(new SelectList(ViewBag.Technicians, "TechnicianID", "Name", Model.TechnicianID.ToString()))">
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Select" class="btn btn-primary" />
    </div>
</form>
