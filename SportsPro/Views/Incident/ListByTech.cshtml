@{
    ViewData["Title"] = "Update Incidents";
}

<h1>Select Technician</h1>

<form asp-action="TechList" method="post">
    <div class="form-group">
        <label name="Technician" class="control-label"></label>
        <select name="TechnicianID" type="number" class="form-control"
                asp-items="@(new SelectList(ViewBag.Technicians, "TechnicianID", "Name"))">
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Select" class="btn btn-primary" />
    </div>
</form>

